  "scripts": {
    "commit": "git-cz",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_ENV=production NODE_PATH=. node dist/main",
    "start:dev": "NODE_ENV=development NODE_PATH=. nest start --watch",
    "start:debug": "NODE_ENV=development NODE_PATH=. nest start --debug --watch",
    "start:staging": "NODE_ENV=staging NODE_PATH=. npm run build && NODE_ENV=staging NODE_PATH=. node dist/main",
    "start:prod": "NODE_ENV=production NODE_PATH=. npm run build && NODE_ENV=production NODE_PATH=. node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
